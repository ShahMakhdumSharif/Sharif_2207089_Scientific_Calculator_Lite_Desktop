<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane fx:controller="com.example.calculator.controller.UserHistoryController" prefHeight="600.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label layoutX="290.0" layoutY="14.0" text="User Calculation History" />
      <TableView fx:id="historyTable" layoutX="14.0" layoutY="50.0" prefHeight="500.0" prefWidth="672.0">
         <columns>
            <TableColumn fx:id="expressionCol" prefWidth="360.0" text="Expression" />
            <TableColumn fx:id="resultCol" prefWidth="312.0" text="Result" />
         </columns>
      </TableView>
      <Button fx:id="UserHistoryBackButton" layoutX="14.0" layoutY="562.0" mnemonicParsing="false" onAction="#handleBack" text="Back" />
      <Button fx:id="UserHistoryLogoutButton" layoutX="600.0" layoutY="562.0" mnemonicParsing="false" onAction="#handleLogout" text="Logout" />
   </children>
</AnchorPane>
